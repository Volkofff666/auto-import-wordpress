{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "aic/trust-bar",
  "title": "Trust Bar",
  "category": "auto-import",
  "icon": "star-filled",
  "description": "Display company advantages with icons",
  "supports": {
    "html": false
  },
  "textdomain": "auto-import-core",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "items": {
      "type": "array",
      "default": [
        {
          "icon": "shield",
          "text": "Warranty guarantee"
        },
        {
          "icon": "clock",
          "text": "Fast delivery"
        },
        {
          "icon": "check",
          "text": "Quality checked"
        }
      ]
    }
  }
}