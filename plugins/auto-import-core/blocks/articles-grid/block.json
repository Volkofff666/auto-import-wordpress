{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "auto-import/articles-grid",
  "title": "Articles Grid",
  "category": "auto-import",
  "icon": "media-document",
  "description": "Display blog articles in grid",
  "attributes": {
    "perPage": {
      "type": "number",
      "default": 3
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "auto-import-core",
  "render": "file:./render.php"
}