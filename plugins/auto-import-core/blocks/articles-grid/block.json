{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "aic/articles-grid",
  "title": "Articles Grid",
  "category": "auto-import",
  "icon": "admin-post",
  "description": "Display a grid of blog articles",
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 3
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showDate": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "auto-import-core",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}