{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "aic/car-grid",
  "title": "Car Grid",
  "category": "auto-import",
  "icon": "grid-view",
  "description": "Display a grid of cars with filters",
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 6
    },
    "filterByStatus": {
      "type": "string",
      "default": ""
    },
    "filterByBrand": {
      "type": "string",
      "default": ""
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "order": {
      "type": "string",
      "default": "DESC"
    }
  },
  "textdomain": "auto-import-core",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}