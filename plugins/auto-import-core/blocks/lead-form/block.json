{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "aic/lead-form",
  "title": "Lead Form",
  "category": "auto-import",
  "icon": "email",
  "description": "Contact form to collect leads",
  "supports": {
    "html": false
  },
  "textdomain": "auto-import-core",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Request a Quote"
    },
    "subtitle": {
      "type": "string",
      "default": "Fill out the form and we will contact you"
    }
  }
}