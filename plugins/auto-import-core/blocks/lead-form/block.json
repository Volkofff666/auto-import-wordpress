{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "aic/lead-form",
  "title": "Lead Form",
  "category": "auto-import",
  "icon": "feedback",
  "description": "Contact form for lead generation",
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Оставьте заявку"
    },
    "subtitle": {
      "type": "string",
      "default": "Мы свяжемся с вами в течение 15 минут"
    }
  },
  "textdomain": "auto-import-core",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}